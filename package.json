{
  "scripts": {
    "test:jest": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:ava": "nyc ava && nyc report --reporter=text-lcov | coveralls"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/jest/**/*.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/aaa.js"
    ]
  },
  "ava": {
    "files": [
      "./test/ava/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "src/bbb.js"
    ]
  },
  "dependencies": {
    "ava": "^2.4.0",
    "codecov": "^3.6.5",
    "jest": "^24.9.0",
    "nyc": "^14.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.7"
  }
}
